{
  "index_name": "psvishnu-blogs",
  "start_urls": ["https://www.vishnups.com"],
  "sitemap_urls": ["https://www.vishnups.com/sitemap.xml"],
  "sitemap_alternate_links": true,
  "stop_urls": ["/search"],
  "selectors": {
    "lvl0": {
      "selector": "(//ul[contains(@class,'menu__list')]//a[contains(@class, 'menu__link menu__link--sublist menu__link--active')]/text() | //label[contains(@class, 'navbarSearchContainer')]/text())[last()]",
      "type": "xpath",
      "global": true,
      "default_value": "Blog"
    },
    "lvl1": "header h1",
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5, article td:first-child",
    "lvl6": "article h6",
    "text": "article p, article li, article td:last-child"
  },
  "custom_settings": {
    "attributesForFaceting": ["type", "lang"]
  }
}
